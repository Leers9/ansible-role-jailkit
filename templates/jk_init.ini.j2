# {{ ansible_managed }}
{% if jk_init is defined %}
{%   for profile in jk_init|sort %}
[{{ profile }}]
{%     for key, value in jk_init[profile].iteritems()|sort %}
{%       if value is iterable and value is not string%}
{{ key }} = {{ value|sort|join(', ') }}
{%       else %}
{{ key }} = {{ value }}
{%       endif %}
{%     endfor %}

{%   endfor %}
{% endif %}
