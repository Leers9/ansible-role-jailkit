# {{ ansible_managed }}
{% if jk_uchroot is defined %}
{%   for profile in jk_uchroot|sort %}
{%     if jk_uchroot[profile]['group'] is defined and jk_uchroot[profile]['group'] %}
[group {{ profile }}]
{%     else %}
[{{ profile }}]
{%     endif %}
{%     for key in jk_uchroot[profile]|sort %}
{%       if key != 'group' %}
{%         if jk_uchroot[profile][key] is number %}
{{ key }} = {{ jk_uchroot[profile][key] }}
{%         elif jk_uchroot[profile][key] is string %}
{{ key }} = {{ jk_uchroot[profile][key] }}
{%         elif jk_uchroot[profile][key] is iterable %}
{{ key }} = {{ jk_uchroot[profile][key]|sort|join(', ') }}
{%         else %}
{{ key }} = {{ jk_uchroot[profile][key] }}
{%         endif %}
{%       endif %}
{%     endfor %}

{%   endfor %}
{% endif %}
