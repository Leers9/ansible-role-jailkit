# {{ ansible_managed }}
{% if jk_lsh is defined %}
{%   for profile in jk_lsh|sort %}
{%     if jk_lsh[profile]['group'] is defined and jk_lsh[profile]['group'] %}
[group {{ profile }}]
{%     else %}
[{{ profile }}]
{%     endif %}
{%     for key in jk_lsh[profile]|sort %}
{%       if key != 'group' %}
{%         if jk_lsh[profile][key] is number or jk_lsh[profile][key] is string %}
{{ key }} = {{ jk_lsh[profile][key] }}
{%         elif jk_lsh[profile][key] is iterable %}
{{ key }} = {{ jk_lsh[profile][key]|sort|join(', ') }}
{%         else %}
{{ key }} = {{ jk_lsh[profile][key] }}
{%         endif %}
{%       endif %}
{%     endfor %}

{%   endfor %}
{% endif %}
